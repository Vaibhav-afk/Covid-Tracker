<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Result">

    <TextView
        android:id="@+id/TextView1"
        android:textStyle="bold"
        android:layout_width="315dp"
        android:layout_height="90dp"
        android:color="#0CB5AA "
        android:text="Current Status"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        app:fontFamily="@font/amita_bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.625"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView"
        android:textStyle="bold"
        android:layout_width="418dp"
        android:layout_height="95dp"
        app:layout_constraintBottom_toTopOf="@+id/text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/header" />

    <ImageView
        android:id="@+id/imageView2"
        android:textStyle="bold"
        android:layout_width="418dp"
        android:layout_height="95dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/footer" />

    <TextView
        android:id="@+id/deaths"
        android:textStyle="bold"
        android:layout_width="178dp"
        android:layout_height="61dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="12dp"
        android:text="People Died:"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ConfirmedForeign" />

    <TextView
        android:id="@+id/confirmedIndian"
        android:textStyle="bold"
        android:layout_width="178dp"
        android:layout_height="61dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="56dp"
        android:text="Indian Confirmed Cases:"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextView1" />

    <TextView
        android:id="@+id/ConfirmedForeign"
        android:textStyle="bold"
        android:layout_width="178dp"
        android:layout_height="61dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:text="Foreign Confirmed Cases:"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/confirmedIndian" />

    <TextView
        android:id="@+id/discharged"
        android:textStyle="bold"
        android:layout_width="178dp"
        android:layout_height="61dp"
        android:layout_marginStart="28dp"
        android:text="Discharged Patients:"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="@+id/dischargedResult"
        app:layout_constraintEnd_toStartOf="@+id/dischargedResult"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/deaths"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/confirmedIndianResult"
        android:gravity="center"
        android:layout_width="140dp"
        android:layout_height="61dp"
        android:layout_marginTop="56dp"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.615"
        app:layout_constraintStart_toEndOf="@+id/confirmedIndian"
        app:layout_constraintTop_toBottomOf="@+id/TextView1" />

    <TextView
        android:id="@+id/confirmedForeignResult"
        android:gravity="center"
        android:layout_width="143dp"
        android:layout_height="61dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/confirmedIndianResult" />

    <TextView
        android:id="@+id/deathsResult"
        android:gravity="center"
        android:layout_width="136dp"
        android:layout_height="61dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="12dp"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toEndOf="@+id/deaths"
        app:layout_constraintTop_toBottomOf="@+id/confirmedForeignResult" />

    <TextView
        android:id="@+id/dischargedResult"
        android:gravity="center"
        android:layout_width="137dp"
        android:layout_height="61dp"
        android:layout_marginEnd="24dp"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/deathsResult"
        app:layout_constraintVertical_bias="0.138" />

</androidx.constraintlayout.widget.ConstraintLayout>